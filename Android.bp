//#######################################
// com.nxp.nfc - library
//#######################################
prebuilt_etc {
    name: "com.nxp.nfc.xml",
    sub_dir: "permissions",
    src: "com.nxp.nfc.xml",
}

java_library {

    name: "com.nxp.nfc",
    installable: true,
    required: ["com.nxp.nfc.xml"],

    srcs: [
        "com/**/I*.aidl",
        "com/**/*.java",
        "android/**/*.java",
    ],

    libs: [
        "framework-nfc.impl",
        "unsupportedappusage"
    ]

}
